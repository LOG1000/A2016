#: src/ui.cpp:259 src/ui.cpp:585
msgid "Software Update"
msgstr "Software Eguneraketa"

#: src/ui.cpp:436
msgid "Release notes:"
msgstr "Argitalpen oharrak:"

#: src/ui.cpp:462
msgid "Skip this version"
msgstr "Jauzi bertsio hau"

#: src/ui.cpp:468
msgid "Remind me later"
msgstr "Gogoratu geroago"

#: src/ui.cpp:473 src/ui.cpp:486
msgid "Install update"
msgstr "Ezarri eguneraketa"

#: src/ui.cpp:584
#, c-format
msgid "%s cannot be restarted."
msgstr "%s ezin da berrabiarazi."

#: src/ui.cpp:587
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Zihurtatu ez duzula gorde gabeko agiririk eta saiatu berriro."

#: src/ui.cpp:594
msgid "Launching the installer..."
msgstr "Ezartzailea abiarazten..."

#: src/ui.cpp:599
msgid "Failed to launch the installer."
msgstr "Hutsegitea ezartzailea abiarazterakoan."

#: src/ui.cpp:621
msgid "Checking for updates..."
msgstr "Eguneraketak egiaztatzen..."

#: src/ui.cpp:623 src/ui.cpp:685 src/ui.cpp:765
msgid "Cancel"
msgstr "Ezeztatu"

#: src/ui.cpp:641
msgid "You're up to date!"
msgstr "Eguneratuta zaude!"

#: src/ui.cpp:648
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s da bertsio eskuragarri berriena."

#: src/ui.cpp:661
msgid "Close"
msgstr "Itxi"

#: src/ui.cpp:680
msgid "Update Error!"
msgstr "Eguneraketa Akatsa!"

#: src/ui.cpp:682
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Akats bat gertatu da eguneraketa argibideak berreskuratzean; zihur zaude internetera elkartuta zaudela? Mesedez saiatu berriro geroago."

#: src/ui.cpp:723
#, c-format
msgid "A new version of %s is available!"
msgstr "%s bertsio berri bat eskuragarri!"

#: src/ui.cpp:726
msgid "Get update"
msgstr "Eguneratu"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:732
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s eskuragarri dago (orain %s duzu). Orain jeistea nahi duzu?"

#: src/ui.cpp:763
msgid "Downloading update..."
msgstr "Eguneraketa jeisten..."

#: src/ui.cpp:791
#, c-format
msgid "%s of %s"
msgstr "%s -> %s-tik"

#: src/ui.cpp:820
msgid "Ready to install."
msgstr "Ezartzeko gertu."

#: src/ui.cpp:960
msgid "Check for updates automatically?"
msgstr "Berezgaitasunez egiaztatu eguneraketak?"

#: src/ui.cpp:970
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "%s-k berez egiaztatu behar ditu eguneraketak? Betik egiaztatu ditzakezu eguneraketak eskuz menutik."

#: src/ui.cpp:984
msgid "Check automatically"
msgstr "Berez egiaztatu"

#: src/ui.cpp:989
msgid "Don't check"
msgstr "Ez egiazta"

