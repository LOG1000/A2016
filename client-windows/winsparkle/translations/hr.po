#: src/ui.cpp:259 src/ui.cpp:585
msgid "Software Update"
msgstr "Ažuriranje softvera"

#: src/ui.cpp:436
msgid "Release notes:"
msgstr "Napomene uz izdanje:"

#: src/ui.cpp:462
msgid "Skip this version"
msgstr "Preskoči ovu verziju"

#: src/ui.cpp:468
msgid "Remind me later"
msgstr "Podsjeti me kasnije"

#: src/ui.cpp:473 src/ui.cpp:486
msgid "Install update"
msgstr "Instaliraj ažuriranje"

#: src/ui.cpp:584
#, c-format
msgid "%s cannot be restarted."
msgstr "%s nije moguće ponovno pokrenuti."

#: src/ui.cpp:587
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Provjeri da ne postoje nespremljeni dokumenti i pokušaj ponovno."

#: src/ui.cpp:594
msgid "Launching the installer..."
msgstr "Pokrećem instalaciju..."

#: src/ui.cpp:599
msgid "Failed to launch the installer."
msgstr "Pokretanje instalacije nije uspjelo."

#: src/ui.cpp:621
msgid "Checking for updates..."
msgstr "Provjera ažuriranja..."

#: src/ui.cpp:623 src/ui.cpp:685 src/ui.cpp:765
msgid "Cancel"
msgstr "Odustani"

#: src/ui.cpp:641
msgid "You're up to date!"
msgstr "Ti si u toku!"

#: src/ui.cpp:648
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s je trenutno najnovija dostupna verzija."

#: src/ui.cpp:661
msgid "Close"
msgstr "Zatvori"

#: src/ui.cpp:680
msgid "Update Error!"
msgstr "Pogreška pri ažuriranju!"

#: src/ui.cpp:682
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Pojavila se pogreška u dohvaćanju ažuriranja; da li si spojen na internet? Pokušaj ponovo kasnije."

#: src/ui.cpp:723
#, c-format
msgid "A new version of %s is available!"
msgstr "Nova verzija %s je dostupna!"

#: src/ui.cpp:726
msgid "Get update"
msgstr "Preuzmi ažuriranje"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:732
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s sada je dostupna (imate %s). Želiš li je sada preuzeti?"

#: src/ui.cpp:763
msgid "Downloading update..."
msgstr "Preuzimam ažuriranje..."

#: src/ui.cpp:791
#, c-format
msgid "%s of %s"
msgstr "%s od %s"

#: src/ui.cpp:820
msgid "Ready to install."
msgstr "Ažuriranje spremno za instalaciju."

#: src/ui.cpp:960
msgid "Check for updates automatically?"
msgstr "Automatska provjera ažuriranja?"

#: src/ui.cpp:970
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Treba %s automatski provjeriti ažuriranja? Uvijek možeš provjeriti ažuriranja ručno iz izbornika."

#: src/ui.cpp:984
msgid "Check automatically"
msgstr "Automatska provjera"

#: src/ui.cpp:989
msgid "Don't check"
msgstr "Ne provjeravaj"

