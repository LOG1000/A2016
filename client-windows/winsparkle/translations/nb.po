#: src/ui.cpp:259 src/ui.cpp:585
msgid "Software Update"
msgstr "Programvareoppdatering"

#: src/ui.cpp:436
msgid "Release notes:"
msgstr "Versjonsnotater:"

#: src/ui.cpp:462
msgid "Skip this version"
msgstr "Hopp over denne versjonen"

#: src/ui.cpp:468
msgid "Remind me later"
msgstr "Minn meg på det senere"

#: src/ui.cpp:473 src/ui.cpp:486
msgid "Install update"
msgstr "Installer oppdatering"

#: src/ui.cpp:584
#, c-format
msgid "%s cannot be restarted."
msgstr "%s kunne ikke bli startet på nytt."

#: src/ui.cpp:587
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Forsikre deg om at du ikke har noen ulagrede dokumenter og prøv igjen."

#: src/ui.cpp:594
msgid "Launching the installer..."
msgstr "Kjører installasjonsprogrammet..."

#: src/ui.cpp:599
msgid "Failed to launch the installer."
msgstr "Kunne ikke kjøre installasjonsprogrammet."

#: src/ui.cpp:621
msgid "Checking for updates..."
msgstr "Ser etter oppdateringer..."

#: src/ui.cpp:623 src/ui.cpp:685 src/ui.cpp:765
msgid "Cancel"
msgstr "Avbryt"

#: src/ui.cpp:641
msgid "You're up to date!"
msgstr "Du er oppdatert!"

#: src/ui.cpp:648
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s er for øyeblikket den nyeste versjonen som er tilgjengelig."

#: src/ui.cpp:661
msgid "Close"
msgstr "Lukk"

#: src/ui.cpp:680
msgid "Update Error!"
msgstr "Oppdateringsfeil!"

#: src/ui.cpp:682
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "En feil oppstod under henting av oppdateringsinformasjon; er du koblet til internett? Prøv igjen senere."

#: src/ui.cpp:723
#, c-format
msgid "A new version of %s is available!"
msgstr "En ny versjon av %s er tilgjengelig!"

#: src/ui.cpp:726
msgid "Get update"
msgstr "Hent oppdatering"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:732
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s er nå tilgjengelig (du har %s). Vil du laste det ned nå?"

#: src/ui.cpp:763
msgid "Downloading update..."
msgstr "Laster ned oppdatering..."

#: src/ui.cpp:791
#, c-format
msgid "%s of %s"
msgstr "%s av %s"

#: src/ui.cpp:820
msgid "Ready to install."
msgstr "Klar for installasjon."

#: src/ui.cpp:960
msgid "Check for updates automatically?"
msgstr "Se etter oppdateringer automatisk?"

#: src/ui.cpp:970
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Skal %s se etter oppdateringer automatisk? Du kan alltid se etter oppdateringer manuelt fra menyen."

#: src/ui.cpp:984
msgid "Check automatically"
msgstr "Se etter automatisk"

#: src/ui.cpp:989
msgid "Don't check"
msgstr "Ikke se etter"

